      {% elif type == 'AVMS_AP' %}     {{ type }}
      {% elif type == 'ALEC' %}        {{ type }}
      {% elif type == 'AVC' %}         {{ type }}
      {% elif type == 'INTEGRATION' %} {{ type }}
      {% elif type == 'INTERNAL' %}    {{ type }}
      {% elif type == 'OTHER' %}       {{ type }}
      {% elif type == 'UFOTA' %}       {{ type }}
      {% endif %}
    &lt;/p&gt;
&lt;/div&gt;{% endraw %}</pre>
